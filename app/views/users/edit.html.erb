<%= form_for @user, method: :put do |f| %>
<p>
	<%= f.label :username %>
	<%= f.text_field :username %>
</p>
<p>
	<%= f.label :fname %>
	<%= f.text_field :fname %>
</p>
<p>
	<%= f.label :email %>
	<%= f.text_field :email %>
</p>
<p>
	<%= f.label :password %>
	<%= f.text_field :password %>
</p>
<p class="profile">
	<%= f.label :bio %>
	<%= f.text_area :bio %>
</p>
<%= f.submit %>
<% end %>