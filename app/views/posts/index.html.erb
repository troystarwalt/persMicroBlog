<h1>Posts#index</h1>
<p> <%= link_to "Throw your own.", posts_new_path %>
<table>
	<div id="list_of_throws">
		<tr>
			<th>Throw</th>
			<th>User</th>
		</tr>
		<% Post.all.each do |post| %>
		<tr>
			<td><%= post.content %></td>
			<td><%= post.user.username %></td>
		</tr>
		<% end %>
	</div>
</table>